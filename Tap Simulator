local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local player = Players.LocalPlayer

-- ===============================
-- VARIABLES
-- ===============================
local EggNames = {}
local SelectedEgg = nil
local running = false

-- ===============================
-- ‡∏î‡∏∂‡∏á‡∏ä‡∏∑‡πà‡∏≠‡πÑ‡∏ü‡∏•‡πå‡∏à‡∏≤‡∏Å workspace.Eggs
-- ===============================
for _, v in ipairs(workspace:WaitForChild("Eggs"):GetChildren()) do
	table.insert(EggNames, v.Name)
end

-- ===============================
-- DROPDOWN
-- ===============================
OverviewSection1:Dropdown({
	Title = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÑ‡∏Ç‡πàü•ö",
	Values = EggNames,
	Value = 1,
	Callback = function(v)
		SelectedEgg = v
		print("‡πÄ‡∏•‡∏∑‡∏≠‡∏Å Egg:", v)
	end
})


-- ===============================
-- TOGGLE
-- ===============================
OverviewSection1:Toggle({
	Title = "‡∏≠‡∏≠‡πÇ‡∏Ñ‡πâ‡∏ã‡∏∑‡πâ‡∏≠‡πÑ‡∏Ç‡πàü•ö",
	Callback = function(v)
		running = v
		if not v then return end

		task.spawn(function()
			-- === CONFIG ===
			local SELECTED_INDEXES = {83}
			local DELAY_TIME = 0

			-- ‡∏´‡∏≤ Folder ‡πÉ‡∏ô ReplicatedStorage ‡πÅ‡∏ö‡∏ö‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á
			local targetFolder
			for _, f in ipairs(ReplicatedStorage:GetChildren()) do
				if f:IsA("Folder") and f:FindFirstChild("Functions") then
					targetFolder = f
					break
				end
			end

			if not targetFolder then
				warn("‡πÑ‡∏°‡πà‡∏û‡∏ö Folder ‡∏ó‡∏µ‡πà‡∏°‡∏µ Functions")
				return
			end

			local functionsFolder = targetFolder:WaitForChild("Functions")
			local allRemotes = functionsFolder:GetChildren()

			print("‡πÉ‡∏ä‡πâ Folder:", targetFolder.Name)
			print("‡∏û‡∏ö Remote:", #allRemotes)

			local args = {
				[1] = SelectedEgg or "‡πÉ‡∏™‡πà‡∏ä‡∏∑‡πà‡∏≠‡πÑ‡∏ü‡∏•‡πå‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å",
				[2] = 3,
				[3] = {}
			}

			while running do
				for _, index in ipairs(SELECTED_INDEXES) do
					local remote = allRemotes[index]
					if remote and remote:IsA("RemoteFunction") then
						pcall(function()
							remote:InvokeServer(unpack(args))
						end)
					end
				end
				task.wait(DELAY_TIME)
			end
		end)
	end
})





