    "1t", "5t", "15t", "35t", "70t", "100t", "500t",
    "1.5qd" -- ‡πÅ‡∏•‡∏∞‡∏≠‡∏∑‡πà‡∏ô‡πÜ ‡∏ï‡∏≤‡∏°‡∏•‡∏≥‡∏î‡∏±‡∏ö‡∏ñ‡πâ‡∏≤‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÄ‡∏û‡∏¥‡πà‡∏°
}

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Index Map ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÅ‡∏õ‡∏•‡∏á‡∏Ñ‡πà‡∏≤ String ‡πÄ‡∏õ‡πá‡∏ô‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç‡∏•‡∏≥‡∏î‡∏±‡∏ö (‡πÄ‡∏ä‡πà‡∏ô "1" -> 1, "5" -> 2)
local valueToIndexMap = {}
for i, v in ipairs(dropValues) do
    valueToIndexMap[v] = i
end

_G.SelectedArgument = 1 -- ‡∏Ñ‡πà‡∏≤ Default
_G.AutoFarm = false

-- === ‡∏™‡πà‡∏ß‡∏ô UI Dropdown === --
OverviewSection1:Dropdown({
    Title = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏Å‡∏≤‡∏£ Rebirths‚ôªÔ∏è",
    Values = dropValues,
    Value = "1", -- ‡∏ï‡πâ‡∏≠‡∏á‡∏ï‡∏£‡∏á‡∏Å‡∏±‡∏ö‡∏Ñ‡πà‡∏≤‡πÉ‡∏ô‡∏ï‡∏≤‡∏£‡∏≤‡∏á dropValues
    Callback = function(selectedValue)
        -- ‡πÅ‡∏õ‡∏•‡∏á‡∏Ñ‡πà‡∏≤ String ‡∏Å‡∏•‡∏±‡∏ö‡πÄ‡∏õ‡πá‡∏ô Index (‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç‡∏•‡∏≥‡∏î‡∏±‡∏ö)
        if valueToIndexMap[selectedValue] then
            _G.SelectedArgument = valueToIndexMap[selectedValue]
            print("Selected Value: " .. selectedValue .. " | Argument Index: " .. _G.SelectedArgument)
        end
    end
})

-- === ‡∏™‡πà‡∏ß‡∏ô UI Toggle ‡πÅ‡∏•‡∏∞ AutoFarm Logic === --
OverviewSection1:Toggle({
    Title = "‡∏≠‡∏≠‡πÇ‡∏ï‡πâ Rebirths‚ôªÔ∏è",
    Default = false,
    Callback = function(Value)
        _G.AutoFarm = Value
        
        if _G.AutoFarm then
            task.spawn(function()
                -- 1. ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÇ‡∏ü‡∏•‡πÄ‡∏î‡∏≠‡∏£‡πå‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥ (‡πÅ‡∏Å‡πâ‡∏õ‡∏±‡∏ç‡∏´‡∏≤ UUID ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô)
                local ReplicatedStorage = game:GetService("ReplicatedStorage")
                local targetFolder = nil
                local functionsFolder = nil

                -- ‡∏ß‡∏ô‡∏•‡∏π‡∏õ‡∏´‡∏≤‡πÇ‡∏ü‡∏•‡πÄ‡∏î‡∏≠‡∏£‡πå‡∏ó‡∏µ‡πà‡∏°‡∏µ‡∏•‡∏π‡∏Å‡∏ä‡∏∑‡πà‡∏≠ "Functions"
                for _, child in pairs(ReplicatedStorage:GetChildren()) do
                    if child:FindFirstChild("Functions") then
                        targetFolder = child
                        functionsFolder = child.Functions
                        break
                    end
                end

                if not functionsFolder then
                    warn("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÇ‡∏ü‡∏•‡πÄ‡∏î‡∏≠‡∏£‡πå Functions! ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏•‡∏≠‡∏á‡πÄ‡∏Ç‡πâ‡∏≤‡πÄ‡∏Å‡∏°‡πÉ‡∏´‡∏°‡πà")
                    _G.AutoFarm = false
                    return
                end

                print("‚úÖ ‡πÄ‡∏à‡∏≠‡πÇ‡∏ü‡∏•‡πÄ‡∏î‡∏≠‡∏£‡πå: " .. targetFolder.Name)
                
                -- ‡∏î‡∏∂‡∏á Remote ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
                local allRemotes = functionsFolder:GetChildren()
                local SELECTED_REMOTE_INDEX = 124 -- ‡∏ï‡∏≤‡∏°‡∏ó‡∏µ‡πà‡∏Ñ‡∏∏‡∏ì‡∏£‡∏∞‡∏ö‡∏∏
                
                -- 2. ‡∏•‡∏π‡∏õ‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô
                while _G.AutoFarm do
                    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï Arguments ‡∏ï‡∏≤‡∏°‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÉ‡∏ô Dropdown ‡∏•‡πà‡∏≤‡∏™‡∏∏‡∏î
                    local arguments = {
                        [1] = _G.SelectedArgument 
                    }

                    local remote = allRemotes[SELECTED_REMOTE_INDEX]
                    
                    if remote and remote:IsA("RemoteFunction") then
                        task.spawn(function()
                            local success, err = pcall(function()
                                -- InvokeServer
                                return remote:InvokeServer(unpack(arguments))
                            end)
                            
                            if success then
                                -- print("üéØ ‡∏¢‡∏¥‡∏á‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à Arg: " .. _G.SelectedArgument) -- ‡πÄ‡∏õ‡∏¥‡∏î‡∏ñ‡πâ‡∏≤‡∏£‡∏Å Console
                            else
                                warn("‚ö†Ô∏è ‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î: " .. tostring(err))
                            end
                        end)
                    end
                    
                    task.wait(0) -- Delay Time (‡πÉ‡∏™‡πà 0 ‡∏Ñ‡∏∑‡∏≠‡πÄ‡∏£‡πá‡∏ß‡∏™‡∏∏‡∏î)
                end
                print("üõë ‡∏´‡∏¢‡∏∏‡∏î‡∏£‡∏∞‡∏ö‡∏ö AutoFarm")
            end)
        else
            print("üõë Toggle ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô")
        end
    end
})





   
end










-- */  Other  /* --
do
    local InviteCode = "dUnSsjeUDF"
    local DiscordAPI = "https://discord.com/api/v10/invites/" .. InviteCode .. "?with_counts=true&with_expiration=true"

    local Response = WindUI.cloneref(game:GetService("HttpService")):JSONDecode(WindUI.Creator.Request({
        Url = DiscordAPI,
        Method = "GET",
        Headers = {
            ["User-Agent"] = "WindUI/Example",
            ["Accept"] = "application/json"
        }
    }).Body)
    
    local DiscordTab = OtherSection:Tab({
        Title = "Discord",
        Icon = "rbxassetid://121030902371363",
    })
    
    if Response and Response.guild then
        DiscordTab:Section({
            Title = "Join our Discord server!",
            TextSize = 20,
        })
        local DiscordServerParagraph = DiscordTab:Paragraph({
            Title = tostring(Response.guild.name),
            Desc = tostring(Response.guild.description),
            Image = "https://cdn.discordapp.com/icons/" .. Response.guild.id .. "/" .. Response.guild.icon .. ".png?size=1024",
            Thumbnail = "https://cdn.discordapp.com/banners/1300692552005189632/35981388401406a4b7dffd6f447a64c4.png?size=512",
            ImageSize = 48,
            Buttons = {
                {
                    Title = "Copy link",
                    Icon = "link",
                    Callback = function()
                        setclipboard("https://discord.gg/" .. InviteCode)
                    end
                }
            }
        })
        
    end
end
